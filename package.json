{
  "name": "web-remote-desktop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsc-watch -p tsconfig.server.json --sourceMap --onSuccess \"tsc-watch -p tsconfig.server.index.json --sourceMap --onFirstSuccess 'cross-env NODE_ENV=development node --enable-source-maps ./dist/server/index.js'\"",
    "build": "tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.13",
    "cross-env": "^7.0.3",
    "iron-session": "^6.2.1",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.4",
    "simple-signal-client": "^3.0.0",
    "simple-signal-server": "^3.0.0",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "swr": "^1.3.0",
    "wrtc": "^0.4.7"
  },
  "devDependencies": {
    "@types/node": "18.8.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "error-handled-node-hmr": "^1.0.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "node-hmr": "^1.3.1",
    "tsc-watch": "^6.0.0",
    "typescript": "4.8.4"
  }
}